@using MiPrimeraAplicacionWeb.Models
@model List<LugarCLS>

@{
    ViewBag.Title = "Index";
}

    <table class="table">


        <thead>
            <tr>
                <td>@Html.LabelFor(p => Model[0].iidlugar)</td>
                <td>@Html.LabelFor(p => Model[0].nombre)</td>
                <td>@Html.LabelFor(p => Model[0].descripcion)</td>
            </tr>
        </thead>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.iidlugar</td>
                <td>@item.nombre</td>
                <td>@item.descripcion</td>
            </tr>
        }


        <tbody>
        </tbody>

        @Html.ActionLink("Adicionar", "Adicionar", null, new { @class = "btn btn-primary" })


    </table>

